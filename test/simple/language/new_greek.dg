(program entry point)
	*(repeat forever)
	(get input $Words)
	(if) (empty $Words) (then) (stop) (endif)
	(collect $Obj)
		(determine object $Obj)
			*(object $Obj)
		(from words)
			(name $Obj)
		(matching all of $Words)
	(into $Options)
	(if) (empty $Options) (then)
		No matches
	(else)
		Matches: (print list $Options)
	(endif)
	(par) (fail)

(print list [$Last])
	$Last
(print list [$Head|$Tail])
	$Head , (print list $Tail)

